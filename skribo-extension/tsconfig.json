{
  "compilerOptions": {
    "lib": [
      "es2015",
      "dom"
    ],
    "target": "es5",
    "module": "none",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "skipLibCheck": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true
  },
  "types": [
    "angular",
    "moment",
    "jquery"
  ],
  "exclude": [
    "node_modules"
  ]
}